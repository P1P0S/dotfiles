{
  "suggest.labelMaxLength": 50,
  "suggest.enablePreview": true,
  "suggest.snippetIndicator": " \ue796",
  "suggest.timeout": 5000,
  "suggest.triggerCompletionWait": 30,
  "suggest.removeDuplicateItems": true,
  "suggest.completionItemKindLabels": {
    "keyword": "\uf1de",
    "variable": "\ue79b",
    "value": "\uf89f",
    "operator": "\u03a8",
    "constructor": "\uf0ad",
    "function": "\u0192",
    "reference": "\ufa46",
    "constant": "\uf8fe",
    "method": "\uf09a",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\uf417",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf40d",
    "color": "\ue22b",
    "property": "\ue624",
    "field": "\uf9be",
    "unit": "\uf475",
    "event": "\ufacd",
    "file": "\uf723",
    "folder": "\uf114",
    "snippet": "\ue60b",
    "typeParameter": "\uf728",
    "default": "\uf29c"
  },
  "diagnostic.level": "hint",
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextCurrentLineOnly": true,
  "diagnostic.virtualTextPrefix": " ▸ ",
  "diagnostic.errorSign": "\uf65b",
  "diagnostic.warningSign": "\uf525",
  "diagnostic.infoSign": "\uf7fb",
  "diagnostic.hintSign": "\uf7d6",
  "diagnostic-languageserver.filetypes": {},
  "diagnostic-languageserver.formatFiletypes": {},
  "signature.triggerSignatureWait": 300,
  "codeLens.enable": true,
  "codeLens.separator": "▸",
  "list.indicator": "❯",
  "list.insertMappings": {
    "<C-c>": "do:exit",
    "<C-p>": "action:preview",
    "<C-j>": "do:next",
    "<C-k>": "do:previous",
    "<C-s>": "do:toggle",
    "<C-f>": "do:switch",
    "<A-x>": "do:toggleMode",
    "<Down>": "prompt:next",
    "<Up>": "prompt:previous",
    "<C-v>": "prompt:paste"
  },
  "list.normalMappings": {
    "<C-c>": "do:exit"
  },
  "coc.preferences.enableMessageDialog": true,
  "coc.preferences.semanticTokensHighlights": false,
  "git.changedSign.text": "▌",
  "git.addedSign.text": "▌",
  "git.removedSign.text": "▁",
  "git.changeRemovedSign.text": "▁",
  "git.topRemovedSign.text": "▔",
  "git.changedDecorator": " ",
  "git.stagedDecorator": " ",
  "git.untrackedDecorator": " ✩",
  "git.conflictedDecorator": "⇡⇣",
  "git.virtualTextPrefix": " \ue729 ",
  "project.rootPatterns": [".git", ".svn", ".root", ".project", ".hg"],
  "snippets.shortcut": "S",
  "snippets.priority": 99,
  "snippets.extends": {
    "cpp": ["c"],
    "javascriptreact": ["javascript"],
    "typescript": ["javascript"]
  },
  "suggest.languageSourcePriority": 98,
  "coc.source.file.shortcut": "F",
  "coc.source.file.priority": 89,
  "coc.source.file.ignoreHidden": false,
  "coc.source.around.shortcut": "A",
  "coc.source.around.priority": 88,
  "coc.source.buffer.shortcut": "B",
  "coc.source.buffer.priority": 87,
  "coc.source.buffer.ignoreGitignore": false,
  "coc.source.syntax.shortcut": "X",
  "coc.source.syntax.priority": 79,
  "coc.source.tag.shortcut": "T",
  "coc.source.tag.priority": 78,
  "coc.source.dictionary.shortcut": "D",
  "coc.source.dictionary.priority": 9,
  "coc.source.dictionary.filetypes": [
    "markdown",
    "text",
    "help",
    "gitcommit",
    "snippets"
  ],
  "coc.source.emoji.shortcut": "E",
  "coc.source.emoji.priority": 8,
  "coc.source.emoji.filetypes": ["markdown", "text"],
  "highlight.document.enable": true,
  "highlight.colors.enable": false,
  "highlight.disableLanguages": [],
  "emmet.includeLanguages": {},
  "imselect.defaultInput": "com.apple.keylayout.ABC",
  "explorer.icon.enableNerdfont": true,
  "explorer.keyMappingMode": "none",
  "explorer.keyMappings.global": {
    "s": "toggleSelection",
    "<leader>": "actionMenu",

    "h": "indentPrev",
    "l": "indentNext",
    "o": ["expandable?", ["expanded?", "collapse", "expand"], "open"],
    "<cr>": "open",
    "t": "open:tab",
    "<bs>": "gotoParent",

    "Y": "copyFilepath",
    "y": "copyFilename",
    "c": "copyFile",
    "x": "cutFile",
    "p": "pasteFile",
    "d": "delete",
    "D": "deleteForever",

    "n": "addFile",
    "N": "addDirectory",
    "r": "rename",

    ".": "toggleHidden",
    "R": "refresh",
    "<C-p>": "preview",

    "?": "help",
    "<esc>": "esc",
    "e": "systemExecute",

    "f": "searchRecursive",

    "gf": "gotoSource:file",
    "gb": "gotoSource:buffer",

    "gk": "gitPrev",
    "gj": "gitNext",
    "ga": "gitStage",
    "gu": "gitUnstage"
  },
  "clangd.enabled": true,
  "clangd.arguments": [
    "--clang-tidy",
    "--completion-style=detailed",
    "--fallback-style=google",
    "--header-insertion=iwyu",
    "--suggest-missing-includes"
  ],
  "cmake.lsp.enable": true,
  "rust-analyzer.server.path": "rust-analyzer",
  "rust-analyzer.updates.channel": "nightly",
  "rust-analyzer.cargo.allFeatures": true,
  "rust-analyzer.inlayHints.enable": false,
  "rust-analyzer.lens.enable": false,
  "python.analysis.typeCheckingMode": "basic",
  "prettier.formatterPriority": 1,
  "sql.database": "mysql",
  "lua.installPreReleases": true,
  "Lua.workspace.library": {
    "/opt/homebrew/share/nvim/runtime/lua": true,
    "/opt/homebrew/share/nvim/runtime/lua/vim": true,
    "/opt/homebrew/share/nvim/runtime/lua/vim/lsp": true,
    "/usr/share/nvim/runtime/lua": true,
    "/usr/share/nvim/runtime/lua/vim": true,
    "/usr/share/nvim/runtime/lua/vim/lsp": true
  },
  "Lua.diagnostics.globals": ["vim"],
  "texlab.path": "texlab",
  "texlab.build.onSave": true,
  "markdown-preview-enhanced.previewTheme": "one-dark.css",
  "markdown-preview-enhanced.enableScriptExecution": true,
  "markdownlint.config": {
    "MD013": false,
    "MD036": false,
    "MD041": false
  }
}
